{
  "name": "naijahacks-alerthy",
  "version": "1.0.0",
  "description": "An app that hacks Citizen Safety in Nigeria",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "nodemon --exec babel-node ./src/index.js",
    "migration": "./node_modules/.bin/sequelize db:migrate",
    "migration:rollback": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "migration:refresh": "npm run migration:rollback && npm run migration",
    "db:seed": "./node_modules/.bin/sequelize db:seed:all",
    "test": "jest",
    "lint": "eslint src && eslint test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sircatalyst/NaijaHacks-Alerthy.git"
  },
  "keywords": [
    "Citizen Safety"
  ],
  "author": "Temitope Bamidele, Odunayo Ogungbure",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sircatalyst/NaijaHacks-Alerthy/issues"
  },
  "homepage": "https://github.com/sircatalyst/NaijaHacks-Alerthy#readme",
  "dependencies": {
    "@babel/runtime": "7.7.4",
    "@hapi/joi": "^16.1.8",
    "@timberio/node": "^0.35.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "morgan": "1.9.1",
    "mysql2": "2.0.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "sequelize": "5.21.2",
    "sequelize-cli": "5.5.1",
    "shortid": "^2.2.15",
    "swagger-ui-express": "4.1.2",
    "timber": "^3.1.3",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "7.7.4",
    "@babel/core": "7.7.4",
    "@babel/node": "7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "7.7.4",
    "@babel/plugin-transform-runtime": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/register": "7.7.4",
    "babel-loader": "8.0.6",
    "eslint": "6.7.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "jest": "24.9.0",
    "nodemon": "2.0.1",
    "prettier": "1.19.1",
    "supertest": "4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
